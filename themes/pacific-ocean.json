{
  "$schema": "vscode://schemas/color-theme",
  "name": "Pacific Ocean",
  "type": "dark",
  "semanticTokenColors": {
    "method": "#A6E22E",
    "variable.defaultLibrary": {
      "foreground": "#6BA9B6",
      "fontStyle": "italic"
    },
    "class.defaultLibrary": {
      "foreground": "#4dd67b",
      "fontStyle": "bold underline"
    },
    "*.async": "#4dd67b",
    "*.static": "#CF882C",
    "variable": "#FFFFFF",
    "*.readonly": "#fff",
    "function": "#A6E22E",
    "string": "#c5d671",
    "parameter": { "fontStyle": "italic bold" }
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": "*",
      "settings": {
        "foreground": "#7FDAFF"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
